# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  email: String!
  screen_id: String!
  screen_name: String!
}

type Query {
  user(id: ID!): User!
}

input NewUser {
  email: String!
  password: String!
  screen_id: String!
  screen_name: String!
}

type Mutation {
  createUser(input: NewUser!): User!
}
